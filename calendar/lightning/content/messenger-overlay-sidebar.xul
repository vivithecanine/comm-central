                        <menuitem id="agenda-context-menu-attend-tentative-dontsend-menuitem"
                                  scope="this-occurrence"
                                  label="&calendar.context.attendance.dontsend.label;"
                                  accesskey="&calendar.context.attendance.dontsend.accesskey;"
                                  respvalue="TENTATIVE"
                                  respmode="NONE"/>
